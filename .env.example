# Yugmi Dashboard Environment Variables

# Frontend Configuration
VITE_APP_NAME=Yugmi Dashboard
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_LETTA_SERVER_URL=http://localhost:8283
VITE_WS_URL=ws://localhost:8283/ws

# Authentication
VITE_JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Features
VITE_ENABLE_ORGANIZATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_BILLING=false

# Development
VITE_DEBUG=true
VITE_LOG_LEVEL=debug